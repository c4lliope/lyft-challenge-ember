<div class="route">
  <div class="inputs">
    {{render "location" origin}}
    <div class="middle">
      <img class="arrow" src="assets/arrow.svg">
      {{input value=driverName class="driver-name"}}
    </div>
    {{render "location" destination}}
  </div>
  {{#if winner}}
  <div class="winner">{{winnerText}}</div>
  {{/if}}
</div>
